{{#if description}}
/**
 * {{{escapeComment description}}}
 */
{{/if}}
export type {{{name}}} = {
	{{#each properties}}
	{{#if description}}
	/**
	 * {{{escapeComment description}}}
	 */
	{{/if}}
	{{>isReadOnly}}{{{camelCase name}}}{{>isRequired}}: {{>type parent=../name}};
	{{/each}}
};
{{#if enums}}
{{#unless @root.useUnionTypes}}

export namespace {{{name}}} {

	{{#each enums}}
	{{#if description}}
	/**
	 * {{{escapeComment description}}}
	 */
	{{/if}}
	export enum {{{name}}} {
		{{#each enum}}
		{{#equals name ''}}
		'' = {{{value}}},
		{{else}}
		{{{name}}} = {{{value}}},
		{{/equals}}
		{{/each}}
	}

	{{/each}}

}
{{/unless}}
{{/if}}
